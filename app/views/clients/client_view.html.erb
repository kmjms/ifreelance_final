<div id="page-content-wrapper"  class=" col-sm-9 col-sm-offset-3">
    <div class="card">
        <div class="card-header">
            <h2>Lista de Freelance
                <small>Freelance relacionados conmigo</small>
            </h2>
        </div>
        <div class="table-responsive">
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th>#</th>
                        <th>Nombre</th>
                        <th>Apellido</th>
                        <th>Email</th>
                        <th>Acciones</th>
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                    <div id= "" style="display:none"></div>
                    <% @freelances.each do |fre| %>
                    <tr>
                        <td><%= fre.id %></td>
                        <td><%= fre.name %></td>
                        <td><%= fre.surname %></td>
                        <td><%= fre.email %></td>
                        <td>
                            <a class="btn bgm-teal waves-effect" href = "<%= clients_path%>/<%= fre.id%>/edit"><i class="zmdi zmdi-edit"></i></a> 
                            <% valuePath= clients_path.to_s + "/" + fre.id.to_s %>
                            <form action="/supports/create_client" method="post" style ="display:inline">
                                    
                                    <input name ="freelance_id" type="text" value="<%=fre.id%>" style="display:none">
                                    <button type="submit" class="btn btn-default btn-icon-text waves-effect"><i class="zmdi zmdi-comments"></i> Sala de chat
                                    </button>
                            </form>
                        </td>
                        <td></td>
                    </tr>
                    <% end %>
                </tbody>
            </table>
        </div>
    </div>
  
        <!-- Formulario -->
       
</div>